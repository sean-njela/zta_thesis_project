{
    "id": null,
    "title": "Python App Observability",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "5s",
    "panels": [
      {
        "type": "timeseries",
        "title": "Request Count",
        "datasource": "Prometheus",
        "targets": [{ "expr": "http_requests_total", "refId": "A" }],
        "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 }
      },
      {
        "type": "logs",
        "title": "Python App Logs",
        "datasource": "Loki",
        "targets": [{ "expr": "{service=\"python-app\"}", "refId": "B" }],
        "gridPos": { "x": 0, "y": 8, "w": 12, "h": 8 }
      },
      {
        "type": "traces",
        "title": "Traces Explorer",
        "datasource": "Tempo",
        "gridPos": { "x": 0, "y": 16, "w": 12, "h": 8 }
      }
    ]
  }
  